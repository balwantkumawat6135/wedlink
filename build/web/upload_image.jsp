<body>
    <div class="container-fluid">
        <div class="row" style="margin-top:150px;">
            <div class="col-lg-2 col-md-3 col-sm-3 col-3"></div>
            <div class="col-lg-8 col-md-6 col-sm-6 col-6 card bg-light">
                <i class="fa fa-upload" style="text-align:center;font-size:100px;"></i>
                <h3 style="text-align:center;">Uplaod Image </h3>
                <input id="file-upload" type="file" class="file-upload-input" accept="image/*" multiple>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-3 col-3"></div>
        </div>
    </div>


</body>
</html>


